{
  "name": "message-board-ui",
  "version": "0.0.10-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 127.0.0.1",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:qa": "ng build -c qa",
    "clean:mock": "rm -rf ./mock",
    "mock:deploy": "npm run clean:mock && ng build --base-href=/messageboard/ && mkdir ./mock && mkdir ./mock/messageboard && cp ./dist/message-board-ui/* ./mock/messageboard/ && php -S 127.0.0.1:4200 -t ./mock",
    "php:local": "npm run build && php -S 127.0.0.1:4200 -t ./dist/message-board-ui/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/forms": "^17.0.4",
    "@angular/localize": "^17.0.5",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.0.4",
    "@angular/router": "^17.0.4",
    "@auth0/auth0-angular": "^2.1.0",
    "@ng-bootstrap/ng-bootstrap": "^16.0.0",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^5.3.0",
    "bootstrap-icons": "^1.10.5",
    "heic2any": "^0.0.4",
    "ngx-image-cropper": "^7.0.2",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular/cli": "~17.0.3",
    "@angular/compiler-cli": "^17.0.4",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.2.2"
  }
}
